def result

pipeline{
    agent any
    stages{
        stage('Test'){
            steps{
                script{
                    sh(script:"python ./test.py > result")
                    cat result
                }
            }
        }
        stage('post'){
            steps{
                if (result == 'True'){
                    echo 'Test successfully'
                }else{
                    echo 'Test fail'
                }
            }
        }
       
    }
    
}
